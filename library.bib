issn = {00129658},
journal = {Ecology},
keywords = {Barro colorado nature monument,Fertilization,Fine litter,Fine roots,Nitrogen,Nutrient limitation,Panama,Phosphorus,Potassium,Tree growth,Tropics},
number = {8},
pages = {1616--1625},
