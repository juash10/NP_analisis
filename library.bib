journal = {Proceedings of the National Academy of Sciences},
number = {27},
pages = {9971--9975},
pmid = {15226500},
title = {{Rice yields decline with higher night temperature from global warming}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.0403720101},
volume = {101},
year = {2004}
