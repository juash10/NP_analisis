file = {:C$\backslash$:/Users/JC/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Schwendenmann, Pendall, Potvin - 2007 - Surface soil organic carbon pools, mineralization and CO2 efflux rates under different land-use.pdf:pdf},
isbn = {1863-5520},
keywords = {central panama,costa-rica,eastern amazonia,forest,matter dynamics,mineralization rate,pasture,pasture installation,plantation,puerto-rico,rain-forest,respiration,soil carbon stocks,soil co(2) efflux,stable isotopes,temporal variation,tropical forests,vegetation},
