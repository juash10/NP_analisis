@phdthesis{Canelos-Gallegos2003,
abstract = {The purpose of this project is the implementation of natural landscaping in the Center American Humid Tropic, through the dissemination and use of 78 native ornamental species of the region; the application of ecological concepts, and the understanding of what is conventional landscaping and natural landscaping, as well as the benefits that of the latter. The project was developed on the campus of EARTH University, located in Mercedes de Gu{\'{a}}cimo, province of Lim{\'{o}}n, in the Atlantic region of Costa Rica. It was demonstrated that the region has very little landscaping culture and where people's perception of landscaping tends to be very conventional, emphasizing orderly and geometric forms, defined limits, introduced species and pruned areas of turf grass and hedges. For the purpose of developing natural landscaping in the region in terms of education and information, a naturally landscaped garden was designed and executed at the hostel of the Permanent Education Program of EARTH University. The objectives of the proposal were achieved, including: the use of the proposed species, reduced implementation labor, a reduction in the area of turf grass, client satisfaction, and the combination in spontaneous and natural way vegetable and non-vegetable elements with the natural and architectural surroundings. The garden provides an unusual and attractive aspect to the institution. Finally, some recommendation are offered for designing and executing natural landscaping in the Central American Region of the humid tropics.},
author = {Canelos-Gallegos, Juan Francisco and Garc{\'{i}}a-Katchor, Andrei},
file = {::},
keywords = {EARTH University,conventional landscaping,ecological concepts,garden,humid tropics,information,introduced plants,native plants,natural landscaping,turf grass areas},
