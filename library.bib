journal = {Perspectives in Plant Ecology, Evolution and Systematics},
keywords = {CANOCO,agement,canoco,conservation man-,conservation management,ecosystem stability,heathland conservation,resilience,resistance},
number = {2},
pages = {142--160},
title = {{Ecosystem stability and resilience: a review of their relevance for the conservation management of lowland heaths}},
url = {http://www.sciencedirect.com/science/article/pii/S1433831904700347{\%}5Cnhttp://linkinghub.elsevier.com/retrieve/pii/S1433831904700347},
volume = {3},
