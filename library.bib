keywords = {genetic variation,herbivore resistance,leaf quality,silver birch},
number = {4},
pages = {707--714},
title = {{Leaf litter decomposition differs among genotypes in a local Betula pendula population}},
volume = {152},
year = {2007}
}
@article{Cote2010,
abstract = {Resilience is usually defined as the capacity of an ecosystem to absorb distur- bance without shifting to an alternative state and losing function and services [1–3]. The concept therefore encompasses two separate processes: resistance—the magnitude of disturbance that causes a change in structure—and recovery—the speed of return to the original structure [4,5]—which are fundamentally different but rarely distinguished. Yet, resilience has become a central concept in the manage- ment of natural ecosystems [6,7]. Many current management actions aim to alle- viate local stressors in an effort to increase ecosystem resilience to global climate change [8,9]. Such a management philos- ophy is premised on the belief that eliminating local drivers of ecological change will increase the ability of an ecosystem to resist future climate distur- bances, its ability to recover from such disturbances, or both [2,6]. Measuring resilience is fraught with difficulties [1,3]. Nevertheless, assessing changes in resil- ience as a result of management action is critical because there is general agreement for the existence of a strong link between resilience and sustainability [10]. Success- fully increasing the resilience of natural systems may therefore have important implications for human welfare in the face of global climate change.},
author = {C{\^{o}}t{\'{e}}, Isabelle M. and Darling, Emily S.},
doi = {10.1371/journal.pbio.1000438},
isbn = {1545-7885},
issn = {15449173},
journal = {PLoS Biology},
number = {7},
pages = {e1000438},
pmid = {20668536},
title = {{Rethinking ecosystem resilience in the face of climate change}},
volume = {8},
year = {2010}
}
