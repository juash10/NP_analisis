isbn = {5152943522},
issn = {01682563},
journal = {Biogeochemistry},
keywords = {Boreal Forest Ecosystems,Carbon Cycle,Carbon-Dioxide Evolution,Climatic-Change,Global Change,Land-Use,Maintenance Respiration,Nitrogen Mineralization,Pine Plantations,Primary Productivity,Root Respiration,Seasonal Pattern,Soil-Co2 Emissions},
