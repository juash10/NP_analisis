pmid = {25179616},
title = {{Easy method to examine single nerve fiber excitability and conduction parameters using intact nonanesthetized earthworms}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4154267{\&}tool=pmcentrez{\&}rendertype=abstract},
