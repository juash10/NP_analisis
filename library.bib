@article{Cusack2011a,
abstract = {Anthropogenic nitrogen (N) deposition is increasing rapidly in tropical$\backslash$nregions, adding N to ecosystems that often have high background N$\backslash$navailability. Tropical forests play an important role in the global$\backslash$ncarbon (C) cycle, yet the effects of N deposition on C cycling in$\backslash$nthese ecosystems are poorly understood. We used a field N-fertilization$\backslash$nexperiment in lower and upper elevation tropical rain forests in$\backslash$nPuerto Rico to explore the responses of above- and belowground C$\backslash$npools to N addition. As expected, tree stem growth and litterfall$\backslash$nproductivity did not respond to N fertilization in either of these$\backslash$nN-rich forests, indicating a lack of N limitation to net primary$\backslash$nproductivity (NPP). In contrast, soil C concentrations increased$\backslash$nsignificantly with N fertilization in both forests, leading to larger$\backslash$nC stocks in fertilized plots. However, different soil C pools responded$\backslash$nto N fertilization differently. Labile (low density) soil C fractions$\backslash$nand live fine roots declined with fertilization, while mineral-associated$\backslash$nsoil C increased in both forests. Decreased soil CO(2) fluxes in$\backslash$nfertilized plots were correlated with smaller labile soil C pools$\backslash$nin the lower elevation forest (R(2) = 0.65, p {\textless} 0.05), and with lower$\backslash$nlive fine root biomass in the upper elevation forest (R(2) = 0.90,$\backslash$np {\textless} 0.05). Our results indicate that soil C storage is sensitive$\backslash$nto N deposition in tropical forests, even where plant productivity$\backslash$nis not N-limited. The mineral-associated soil C pool has the potential$\backslash$nto respond relatively quickly to N additions, and can drive increases$\backslash$nin bulk soil C stocks in tropical forests.},
author = {Cusack, Daniela F. and Silver, Whendee L. and Torn, Margaret S. and McDowell, William H.},
doi = {10.1007/s10533-010-9496-4},
file = {:C$\backslash$:/Users/JC/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Cusack et al. - 2011 - Effects of nitrogen additions on above- and belowground carbon dynamics in two tropical forests.pdf:pdf},
isbn = {0168-2563},
issn = {01682563},
journal = {Biogeochemistry},
keywords = {Aboveground biomass,Dissolved organic carbon,Nutrient limitation,Roots,Soil density fractions,Soil respiration},
number = {1-3},
pages = {203--225},
title = {{Effects of nitrogen additions on above- and belowground carbon dynamics in two tropical forests}},
volume = {104},
year = {2011}
}
